{
  "name": "react-service-injector",
  "version": "0.1.0",
  "description": "Dependency Injection for React",
  "main": "index.js",
  "repository": "tgroshon/react-service-injector",
  "author": "tgroshon",
  "license": "MIT",
  "module": "dist/react-service-injector.mjs",
  "sideEffects": false,
  "scripts": {
    "test": "is-ci \"test:ci\" \"test:dev\"",
    "test:dev": "jest --watch",
    "test:ci": "jest",
    "build": "NODE_ENV=production rollup -c",
    "now-build": "yarn && cd www && yarn && yarn build",
    "start": "rollup -c -w",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test:ci",
    "release": "yarn publish",
    "releaseNext": "yarn publish --tag next",
    "format": "prettier {src,src/**,example/src,example/src/**}/*.{md,js,jsx,tsx} --write"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.2.7",
    "lodash.pick": "^4.4.0",
    "prettier": "^2.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-jscc": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^5.2.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  }
}
