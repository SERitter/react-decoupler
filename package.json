  "repository": {
    "url": "https://github.com/testdouble/react-decoupler",
    "type": "git"
  },
  "sideEffects": false,
  "scripts": {
    "format": "prettier {src,src/**,example/src,example/src/**}/*.{md,js,jsx,tsx} --write",
    "postversion": "git push --follow-tags",
    "prepare": "cross-env NODE_ENV=production rollup -c",
